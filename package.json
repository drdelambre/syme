{
  "name": "syme",
  "version": "1.3.1",
  "description": "A model set for allowing you to share data contracts between your view and the rest of your application, take your props on the go",
  "author": "Alex Boatwright <drdelambre@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/drdelambre/syme.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "model",
    "data",
    "binding",
    "server"
  ],
  "dependencies": {
    "continuation-local-storage": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "^3.3.0",
    "chalk": "^1.1.1",
    "enzyme": "^2.3.0",
    "eslint": "3.10.2",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "1.0.0",
    "karma": "1.3.0",
    "karma-chai": "^0.1.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "3.1.2",
    "phantomjs": "2.1.7",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "sinon": "^1.17.1",
    "webpack": "1.12.2"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": true
        }
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      [
        "module-alias",
        [
          {
            "src": "./src",
            "expose": "base"
          },
          {
            "src": "./src/internal",
            "expose": "internal"
          }
        ]
      ],
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error"
          ]
        }
      ],
      "transform-regenerator",
      "transform-react-constant-elements",
      "transform-react-inline-elements"
    ],
    "compact": true
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint -c ./tests/config/lint.json --ext .js --ext .jsx --ignore-path ./tests/config/lint-ignore ./src/",
    "build": "$(npm bin)/babel -d dist/ src/",
    "test": "npm run lint && ./node_modules/.bin/karma start ./tests/config/karma.conf.js --single-run"
  }
}
